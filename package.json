    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
